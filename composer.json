{
    "name": "minitryofjustice/doctrine-migration-version-checker",
    "description": "A small library to interact with doctrine's configuration object to retrieve information about migration version",
    "license": "MIT",
    "require": {
        "php": ">=7.0",
        "doctrine/migrations": "^1.5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0.0",
        "satooshi/php-coveralls": "^1.0.1"
    },
    "authors": [
        {
            "name": "Ali Bahman",
            "email": "abn@webit4.me"
        }
    ],
    "autoload": {
        "psr-4": {
            "Minitryofjustice\\DoctrineMigrationVersionChecker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Minitryofjustice\\DoctrineMigrationVersionCheckerTest\\": "test/"
        }
    }
}
